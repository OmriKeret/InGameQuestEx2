// room script file

function room_Load()
{
player.x = -40;
player.y = 0;
gMenuSlider.Visible = false;
gIconbar.Visible = false;
gMainMenu.Visible = true;
mouse.UseModeGraphic(eModePointer);
}

function room_AfterFadeIn()
{
 
  aMenu.Play();
}

function room_Leave()
{
  gMainMenu.Visible = false;
  aMenu.Stop();
}
